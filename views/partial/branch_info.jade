- console.log(branch["yeed"]);
.panel.panel-info
    .panel-heading
        h2.panel-title
            i.fas.fa-code-branch
            |  Branch Information
    .panel-body
        table.table.table-bordered
            tr
                th.info
                    i.fa.fa-location-arrow
                    |  Network Address
                td
                    span.hash  #{branch_address}
            tr
                th.info
                    i.fas.fa-tag
                    |  Name
                td
                    span=  branch.name
            tr
                th.info
                    i.far.fa-sticky-note
                    |  Description
                td
                    span=  branch.meta.description
            if branch.type != 'immutable'
                tr
                    th.info
                        i.far.fa-star
                        |  Reputation Score
                    td
                        span.number #{sacred[branch_address]}
                tr
                    th.info
                        i.fas.fa-cubes
                        |  YEED
                    td
                        span.number #{yeed}
            tr
                th.info
                    i.fas.fa-archive
                    |  Type
                td
                    span=  branch.type

